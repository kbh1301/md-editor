    --md-syntax-meta-diff-range: #d2a8ff;
    --md-syntax-brackethighlighter-angle: #8b949e;
    --md-syntax-sublimelinter-gutter-mark: #484f58;
    --md-syntax-constant-other-reference-link: #a5d6ff;
    --md-fg-default: #e0e6ed;
    --md-fg-muted: #a0a9b8;
    --md-fg-subtle: #666f7a;
    --md-canvas-default: #343d46;
    --md-canvas-subtle: #404b56;
    --md-border-default: #49525b;
    --md-border-muted: #3a444d;
    --md-neutral-muted: rgba(136, 145, 155, 0.4);
