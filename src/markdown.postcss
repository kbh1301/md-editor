    --md-accent-emphasis: #2f7fdc;
    --md-attention-subtle: rgba(187, 128, 9, 0.25);
    --md-danger-fg: #ff6b66;

    /*
    Original GitHub Dark color schemes:
    
    --md-fg-default: #c9d1d9;
    --md-fg-muted: #8b949e;
    --md-fg-subtle: #484f58;
    --md-canvas-default: #0d1117;
    --md-canvas-subtle: #161b22;
    --md-border-default: #30363d;
    --md-border-muted: #21262d;
    --md-neutral-muted: rgba(110, 118, 129, 0.4);
    --md-accent-fg: #58a6ff;
    --md-accent-emphasis: #1f6feb;
    --md-attention-subtle: rgba(187, 128, 9, 0.15);
    --md-danger-fg: #f85149;
    */
}

.markdown-body {
    -ms-text-size-adjust: 100%;
