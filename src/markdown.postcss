    --md-syntax-carriage-return-text: #f0f6fc;
    --md-syntax-carriage-return-bg: #b62324;
    --md-syntax-string-regexp: #7ee787;
    --md-syntax-markup-list: #f2cc60;
    --md-syntax-markup-heading: #1f6feb;
    --md-syntax-markup-italic: #c9d1d9;
